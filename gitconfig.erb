[user]
	name  = <%=  print(" - Your Name: ");  STDOUT.flush; STDIN.gets.chomp %>
	email = <%= print(" - Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[credential]
	helper = osxkeychain
[core]
  excludesfile = ~/.gitignore
  pager = /usr/local/share/git-core/contrib/diff-highlight/diff-highlight | less -R
[color]
  ui=true
[color "branch"]
  current = green
  remote = red
[color "status"]
  added = green
  changed = red
  untracked = cyan
[color "diff"]
  frag = cyan
  old = red
  new = green
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold 52
  newNormal = green bold
  newHighlight = green bold 22
[push]
	default = current
[instaweb]
	local = true
	httpd = webrick
[alias]
	s   = status --short --branch
	l   = log --parents
	ll  = log -p --parents
	who = shortlog -s
	d   = diff --color
	ds  = diff --color --stat
	dw  = diff --color-words
	dd  = --paginate difftool --no-prompt --extcmd=icdiff
	dlt = diff lib/ test/
	a   = add
	ap  = app -p
	aa  = add --all .
	alt = add lib/ test/
	c   = commit
	cm  = commit -m
	k   = checkout
	kb  = checkout -b
	b   = branch -a -v
	bd  = branch -d
	bdd = branch -D
	m   = merge
	p   = push
	pt  = push --tags
	u   = pull --ff-only
	f   = fetch
	rb  = rebase
	chp = cherry-pick
	pn  = remote prune origin
	iw  = instaweb
	iws = instaweb --stop
